<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:vm="clr-namespace:MyCalc.VM"
                    xmlns:ui="clr-namespace:MyCalc.UI" xmlns:mvvm="clr-namespace:MVVM;assembly=MVVM">
    <DataTemplate DataType="{x:Type vm:TabsVM}">
        <ui:TabControl/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:TestVM}">
        <ui:TestUserControl/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:DistributionVM}">
        <ui:DistributionControl/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:DistributionItemVM}">
        <ui:VerticalBarControl/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:HeaderOverlayVM}">
        <mvvm:OverlayControl/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:CardVM}">
        <ui:CardControl/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:CardChooserVM}">
        <ui:CardChooser/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:CardListVM}">
        <ui:CardsListControl/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:CardListDistributionVM}">
        <ui:CardListDistributionControl/>
    </DataTemplate>
</ResourceDictionary>